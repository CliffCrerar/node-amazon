{
  "name": "node-amazon",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Suman Bhattarai",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run dev-server\" \"ng serve --proxy-config proxy.conf.json\"",
    "dev-server": "node server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "compile:server": "tsc -p server.tsconfig.json",
    "lint": "node_modules/eslint/bin/eslint.js --ext .js ./",
    "lint-ts": "node_modules/tslint/bin/tslint --project tslint.json 'src/**/**.ts'",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng run amazon:server:production",
    "migrate": "node scripts/migration.js",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "CI=true concurrently \"npm run lint\" \"npm run lint-ts\""
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.10",
    "@angular/common": "^7.2.10",
    "@angular/compiler": "^7.2.10",
    "@angular/core": "^7.2.10",
    "@angular/forms": "^7.2.10",
    "@angular/http": "^7.2.10",
    "@angular/platform-browser": "^7.2.10",
    "@angular/platform-browser-dynamic": "^7.2.10",
    "@angular/platform-server": "^7.2.10",
    "@angular/router": "^7.2.10",
    "@ng-bootstrap/ng-bootstrap": "4.1.0",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "@ngx-loading-bar/core": "^2.1.2",
    "angular2-notifications": "^1.0.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "bootstrap": "4.3.1",
    "chalk": "^2.3.1",
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "express-jwt": "^5.3.0",
    "express-session": "^1.15.3",
    "express-sslify": "^1.2.0",
    "jsonwebtoken": "^7.4.1",
    "jwt-decode": "^2.2.0",
    "ngx-pagination": "^3.2.1",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.9.0",
    "route-cache": "^0.4.3",
    "rxjs": "^6.4.0",
    "ts-helpers": "^1.1.1",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.6",
    "@angular/cli": "7.3.6",
    "@angular/compiler-cli": "^7.2.10",
    "@types/jasmine": "^2.5.51",
    "@types/node": "^8.0.30",
    "angular2-jwt": "git://github.com/sumanbh/angular2-jwt.git",
    "codelyzer": "^4.5.0",
    "concurrently": "^3.5.1",
    "eslint": "5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.1.2",
    "intl": "^1.2.5",
    "jasmine-core": "^2.6.2",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
